{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "ng-learning-stock-manage-web-site",
    "ejected": true
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "../node_modules/lee-metronic/dist/global/plugins/font-awesome/css/font-awesome.css",
        "../node_modules/lee-metronic/dist/global/plugins/simple-line-icons/simple-line-icons.css",
        "../node_modules/lee-metronic/dist/global/plugins/bootstrap/css/bootstrap.css",
        "../node_modules/lee-metronic/dist/global/plugins/bootstrap-switch/css/bootstrap-switch.css",
        "../node_modules/lee-metronic/dist/global/css/components-rounded.css",
        "../node_modules/lee-metronic/dist/global/css/plugins.css",
        "../node_modules/lee-metronic/dist/layouts/layout2/css/layout.css",
        "../node_modules/lee-metronic/dist/layouts/layout2/css/themes/blue.css",
        "styles.css"
      ],
      "scripts": [
        "../node_modules/lee-metronic/dist/global/plugins/jquery.min.js",
        "../node_modules/lee-metronic/dist/global/plugins/bootstrap/js/bootstrap.js",
        "../node_modules/lee-metronic/dist/global/plugins/js.cookie.min.js",
        "../node_modules/lee-metronic/dist/global/plugins/jquery-slimscroll/jquery.slimscroll.min.js",
        "../node_modules/lee-metronic/dist/global/plugins/jquery.blockui.min.js",
        "../node_modules/lee-metronic/dist/global/plugins/bootstrap-switch/js/bootstrap-switch.min.js",
        "../node_modules/lee-metronic/dist/global/scripts/app.js",
        "../node_modules/lee-metronic/dist/layouts/layout2/scripts/layout.js",
        "../node_modules/lee-metronic/dist/layouts/layout2/scripts/demo.min.js",
        "../node_modules/lee-metronic/dist/layouts/global/scripts/quick-sidebar.min.js",
        "../node_modules/lee-metronic/dist/layouts/global/scripts/quick-nav.min.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "src/tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {
    }
  }
}
